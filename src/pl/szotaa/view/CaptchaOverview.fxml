      <Label fx:id="welcomeText" layoutX="151.0" layoutY="44.0" text="Preview will be displayed here after clicking generate" />
      <Spinner fx:id="numOfDigits" amountToStepBy="1" initialValue="5" layoutX="124.0" layoutY="120.0" max="10" min="0">
      </Spinner>
   </children>
</AnchorPane>
